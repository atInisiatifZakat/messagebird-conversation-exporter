{
    "name": "inisiatif/mbe",
    "description": "MessageBird conversations exporter.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "~8.0.0",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/database": "^9.32",
        "illuminate/http": "^9.32",
        "laravel-zero/framework": "^9.1.3",
        "nunomaduro/termwind": "^1.14"
    },
    "require-dev": {
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^1.2",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^1.22.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": [
        "mbe"
    ]
}
